#include "main.h"
/**
 * _memset - fill a block of memory with a specific value
 * @e: starting address of memory to be filled
 * @f: the desired value
 * @w: number of bytes to be changed
 *
 * Return: changed array with new value for n bytes
 */
char *_memset(char *e, char f, unsigned int w)
{
	int i = 0;

	for (; w > 0; i++)
	{
		e[i] = f;
		w--;
	}
	return (e);
}

